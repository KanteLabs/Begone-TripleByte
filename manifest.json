{
  "name": "Begone Triplebyte",
  "description": "Removes Triplebyte ads from Reddit",
  "author": "KanteLabs",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["https://www.reddit.com/*, https://old.reddit.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}